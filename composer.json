{
    "name": "overconda/licensing-api",
    "description": "Headless License Validation API for WordPress plugins (Envato)",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": "cp -n .env.example .env 2>/dev/null || true",
        "post-update-cmd": "cp -n .env.example .env 2>/dev/null || true"
    }
}
